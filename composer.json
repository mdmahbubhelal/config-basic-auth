{
    "name": "mdmahbubhelal/config-basic-auth",
    "description": "Provides basic auth using a config file for credentials",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "config",
        "basic auth"
    ],
    "authors": [
        {
            "name": "Md Mahbub Helal",
            "email": "mdhelal00@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "MdMahbubHelal\\ConfigBasicAuth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MdMahbubHelal\\ConfigBasicAuth\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MdMahbubHelal\\ConfigBasicAuth\\ConfigBasicAuthServiceProvider"
            ]
        }
    },
    "require-dev": {
        "orchestra/testbench": "^7.0",
        "phpunit/phpunit": "^9.5"
    }
}
